{
  "name": "emapi",
  "license": "MIT",
  "version": "1.0.2",
  "description": "Electron app which hosts electron-gqlmapi with GraphiQL",
  "main": "./src/startElectron.js",
  "homepage": "./",
  "repository": "github:microsoft/eMAPI",
  "bugs": {
    "url": "https://github.com/microsoft/eMAPI/issues"
  },
  "keywords": [
    "electron",
    "graphql",
    "mapi"
  ],
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "electron-dev": "concurrently -k -s first \"npm start\" \"wait-on http://localhost:3000/ && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "electron": "~11.2.0",
    "electron-gqlmapi": "github:microsoft/electron-gqlmapi"
  },
  "devDependencies": {
    "concurrently": "~5.3.0",
    "electron-is-dev": "^1.2.0",
    "graphiql": "~1.3.0",
    "graphql": "~15.4.0",
    "react": "~16.8.0",
    "react-dom": "~16.8.0",
    "react-scripts": "^4.0.3",
    "typescript": "~3.2.0",
    "wait-on": "~5.2.0"
  }
}
